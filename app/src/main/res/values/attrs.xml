<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PlayPauseView">
        <attr name="isCircleDraw" format="boolean" />
        <attr name="circleAlpha" format="integer" />
        <attr name="backgroundColor" format="reference|color" />
        <attr name="drawableColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="PlayingView">
        <attr name="pointerColor" format="color" />
        <attr name="pointerNum" format="integer" />
        <attr name="pointerWidth" format="float" />
        <attr name="pointerDrawDelay" format="integer" />
    </declare-styleable>

</resources>